<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="rectangle">
            <solid android:color="@color/colorPrimary" />

            <size android:width="280dp" android:height="200dp"/>
        </shape>
    </item>

    <item android:left="10dp" android:bottom="10dp" android:right="10dp" android:top="10dp">
        <shape
            android:shape="rectangle">
            <solid android:color="@color/colorPrimary" />
            <size android:height="50dp" android:width="280dp"/>
            <stroke android:color="@color/colorBlack" android:width="3dp"/>
        </shape>
    </item>
    <item android:top="2dp" android:bottom="40dp" android:right="160dp" android:left="20dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/colorPrimary"/>
            <size android:width="120dp" android:height="20dp"/>
        </shape>
    </item>


</layer-list>